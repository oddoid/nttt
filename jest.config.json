{
  "testPathIgnorePatterns": ["/node_modules/", "/dist/"],
  "transform": {".+\\.ts$": "ts-jest"},
  "collectCoverage": true,
  "coverageDirectory": "dist/coverage",
  "errorOnDeprecated": true,
  "coverageThreshold": {
    "global": {
      "statements": "99",
      "branches": 94,
      "functions": 88,
      "lines": 100
    }
  }
}
